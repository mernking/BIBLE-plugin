#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/david/Documents/projects/works/plugins/BIBLE plugin/capacitorHTML/node_modules/.pnpm/@capacitor+cli@7.4.3/node_modules/@capacitor/cli/bin/node_modules:/home/david/Documents/projects/works/plugins/BIBLE plugin/capacitorHTML/node_modules/.pnpm/@capacitor+cli@7.4.3/node_modules/@capacitor/cli/node_modules:/home/david/Documents/projects/works/plugins/BIBLE plugin/capacitorHTML/node_modules/.pnpm/@capacitor+cli@7.4.3/node_modules/@capacitor/node_modules:/home/david/Documents/projects/works/plugins/BIBLE plugin/capacitorHTML/node_modules/.pnpm/@capacitor+cli@7.4.3/node_modules:/home/david/Documents/projects/works/plugins/BIBLE plugin/capacitorHTML/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/david/Documents/projects/works/plugins/BIBLE plugin/capacitorHTML/node_modules/.pnpm/@capacitor+cli@7.4.3/node_modules/@capacitor/cli/bin/node_modules:/home/david/Documents/projects/works/plugins/BIBLE plugin/capacitorHTML/node_modules/.pnpm/@capacitor+cli@7.4.3/node_modules/@capacitor/cli/node_modules:/home/david/Documents/projects/works/plugins/BIBLE plugin/capacitorHTML/node_modules/.pnpm/@capacitor+cli@7.4.3/node_modules/@capacitor/node_modules:/home/david/Documents/projects/works/plugins/BIBLE plugin/capacitorHTML/node_modules/.pnpm/@capacitor+cli@7.4.3/node_modules:/home/david/Documents/projects/works/plugins/BIBLE plugin/capacitorHTML/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@capacitor+cli@7.4.3/node_modules/@capacitor/cli/bin/capacitor" "$@"
else
  exec node  "$basedir/../.pnpm/@capacitor+cli@7.4.3/node_modules/@capacitor/cli/bin/capacitor" "$@"
fi
